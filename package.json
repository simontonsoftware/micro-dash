{
  "name": "micro-dash",
  "version": "1.0.0",
  "description": "Lightweight implementations of simplified lodash functions",
  "main": "./bundles/micro-dash.umd.js",
  "module": "./micro-dash.es5.js",
  "es2015": "./micro-dash.js",
  "typings": "./micro-dash.d.ts",
  "author": "Simonton Software",
  "repository": "simontonsoftware/micro-dash",
  "license": "MIT",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean:all": "yarn clean:lib && yarn clean:sizes && yarn clean:integration",
    "clean:lib": "cd lib && yarn clean",
    "clean:sizes": "cd sizes && yarn clean",
    "clean:integration": "cd integration && yarn clean",
    "install:all": "yarn install:lib && yarn install:sizes && yarn install:integration",
    "install:lib": "cd lib && yarn install",
    "install:sizes": "cd sizes && yarn install",
    "install:integration": "cd integration && yarn install",
    "build:all": "yarn build:lib && yarn build:sizes && yarn build:integration",
    "build:lib": "cd lib && yarn build",
    "build:sizes": "cd sizes && yarn build",
    "build:integration": "cd integration && yarn build",
    "test:all": "yarn test:lib && yarn test:integration",
    "test:lib": "cd lib && yarn test",
    "test:integration": "cd integration && yarn e2e && yarn e2e:aot",
    "release": "standard-version && yarn build:lib"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "camelcase": "^4.0.0",
    "cpx": "^1.5.0",
    "glob": "^7.1.1",
    "lodash-es": "^4.17.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "standard-version": "^4.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
