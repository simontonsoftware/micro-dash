{
  "name": "micro-dash-platform",
  "version": "5.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sizes": "ng build micro-dash --prod && ng build sizes --prod && cd projects/sizes && ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" explore",
    "docs": "cd docs && typedoc ../projects/micro-dash/src",
    "prettier-all": "prettier --write \"**/*.{html,js,json,md,scss,ts,yml}\"",
    "release": "yarn docs && git add docs/* && standard-version --commit-all && yarn build micro-dash --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "utility-types": "^3.4.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "~8.9.4",
    "@types/prettier": "^1.13.2",
    "@types/sinon": "^5.0.1",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash-es": "^4.17.10",
    "ng-packagr": "^4.4.0",
    "prettier": "^1.15.1",
    "protractor": "~5.4.0",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.0",
    "s-ng-test-utils": "^0.1.0",
    "sinon": "^7.1.1",
    "source-map-explorer": "^1.5.0",
    "standard-version": "^4.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "~3.1.6"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "standard-version": {
    "scripts": {
      "postbump": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" standard-version-postbump",
      "postchangelog": "prettier --write CHANGELOG.md",
      "precommit": "git add projects/micro-dash/package.json"
    }
  }
}
