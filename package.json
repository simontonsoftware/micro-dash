{
  "name": "micro-dash-platform",
  "version": "5.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sizes": "ng build micro-dash --prod && ng build sizes --prod && cd projects/sizes && ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" explore",
    "docs": "cd docs && typedoc ../projects/micro-dash/src",
    "prettier-all": "prettier --write \"**/*.{html,js,json,md,scss,ts,yml}\"",
    "release": "yarn docs && git add docs/* && standard-version --commit-all && yarn build micro-dash --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.2",
    "@angular/common": "~8.0.0-rc.2",
    "@angular/compiler": "~8.0.0-rc.2",
    "@angular/core": "~8.0.0-rc.2",
    "@angular/forms": "~8.0.0-rc.2",
    "@angular/platform-browser": "~8.0.0-rc.2",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.2",
    "@angular/router": "~8.0.0-rc.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "utility-types": "^3.7.0",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.2",
    "@angular-devkit/build-ng-packagr": "~0.800.0-rc.2",
    "@angular/cli": "~8.0.0-rc.3",
    "@angular/compiler-cli": "~8.0.0-rc.2",
    "@angular/language-service": "~8.0.0-rc.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "~8.9.4",
    "@types/prettier": "^1.16.3",
    "@types/sinon": "^7.0.11",
    "codelyzer": "^5.0.0",
    "coveralls": "^3.0.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lodash-es": "^4.17.10",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.2",
    "s-ng-test-utils": "^0.1.0",
    "sinon": "^7.3.2",
    "source-map-explorer": "^1.8.0",
    "standard-version": "^6.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.3"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "standard-version": {
    "scripts": {
      "postbump": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" standard-version-postbump",
      "postchangelog": "prettier --write CHANGELOG.md",
      "precommit": "git add projects/micro-dash/package.json"
    }
  }
}
